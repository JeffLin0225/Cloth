package com.ispan.demo.model;

import java.util.Objects;

import jakarta.persistence.Entity;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;

@Getter
@Setter
@NoArgsConstructor
@Entity
public class CartId {
	
	private Users users_id;
	private Product product_id;
	private Order order_id;
	@Override
	public int hashCode() {
		return Objects.hash(order_id, product_id, users_id);
	}
	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		CartId other = (CartId) obj;
		return Objects.equals(order_id, other.order_id) && Objects.equals(product_id, other.product_id)
				&& Objects.equals(users_id, other.users_id);
	}
	
	
}
